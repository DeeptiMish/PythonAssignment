    "\n",
    "Ans- x = int(input(\"Enter value of x in integer: \"))\n",
    "     y = int(input(\"Enter value of y in integer: \"))\n",
    "     x = x + y\n",
    "     y = x - y\n",
    "     x = x - y\n",
    "     print(\"x =\", x)\n",
    "     print(\"y =\", y)"
